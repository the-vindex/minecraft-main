local FULL_STACK = 64
local chest = peripheral.wrap("bottom")
print(chest)
while true do
	turtle.select(1)
	turtle.suckUp()
	sleep(5)
	
	if (not(rs.testBundledInput("front",colors.blue))) then
		local turtleCount = turtle.getItemCount(1)
		print(turtleCount)
		if (turtleCount < FULL_STACK) then
			chest.condenseItems()
			chest.pushItem("up",1,64-turtleCount,1)
		end
	end
	
	turtle.dropUp()
	sleep(5)
end

local FULL_STACK = 64
local chest = peripheral.wrap("bottom")
print(chest)
while true do
	turtle.select(1)
	turtle.suckUp()
	sleep(5)
	
	local turtleCount = turtle.getItemCount(1)
	print(turtleCount)
	if (turtleCount < FULL_STACK) then
		chest.condenseItems()
		chest.pushItem("up",0,64-turtleCount,0)
	end
	
	turtle.dropUp()
	sleep(5)
end
